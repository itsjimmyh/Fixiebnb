<div class="container-fluid" style="width: 300px;">
  <div class='row float-left'>
    <h2>Your Listings</h2>
  </div>
    <div class="row" style="position: relative; height: 300px; width: 100%;">
      <% images.forEach(function (image) { %>
          <a href="/#/listing/requests/<%= image.escape('id') %>">
            <img src="<%= image.escape('feature_img') %>" class="img-responsive img-thumbnail float-left"></img>
          </a>

      <% }) %>
    </div>
</div>
