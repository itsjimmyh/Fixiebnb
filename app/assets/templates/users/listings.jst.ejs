<div class='span8 main text-center'>
  <h2>Your Listings</h2>

  <div class="container-fluid">
    <div class="row" style="position: relative;">
      <% images.forEach(function (image) { %>
        <div class="col-xs-6" style="width: 400px;">
          <a href="/#/listing/<%= image.escape('id') %>">
            <img src="<%= image.escape('feature_img') %>" class="img-responsive"></img>
          </a>
        </div>
      <% }) %>
    </div>
</div>
